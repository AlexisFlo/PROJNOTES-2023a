{
    "presets" : [
        [
            "minify",{
                "builtIns": false,
                "evaluate" : false,
                "mangle" : false
            }
        ],
        [
            "@babel/preset-env",{
                // Los targets son las plataformas donde se ejecutara el c√≥digo final
                "targets" : {"node" : "current"}
            }
        ]
    ],
    "plugins": [
        [
            "module-resolver", {
                "root":["./"],
                "alias": {
                    "@server" : "./server"
                }
            }
        ]
    ]
}